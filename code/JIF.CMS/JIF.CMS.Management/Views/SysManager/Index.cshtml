
@{
    var managers = ViewBag.Managers as IList<JIF.CMS.Core.Domain.SysAdmin>;

}


<div class="row">
    <div class="col-md-12">



        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">系统用户管理</h3>
            </div>

            <!-- /.box-header -->
            <div class="box-body">
                <table class="table table-bordered table-hover table-condensed table-striped">
                    <thead>
                        <tr>
                            <th style="width: 30px;">#</th>
                            <th style="width: 50px;">编号</th>
                            <th>账号</th>
                            <th>邮箱</th>
                            <th>电话</th>
                            <th>启用</th>
                            <th>创建时间</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var manager in managers)
                        {
                            <tr class="no-padding">
                                <td>
                                    <input type="checkbox" />
                                </td>
                                <td>@manager.Id</td>
                                <td>@manager.Account</td>
                                <td>@manager.Email</td>
                                <td>@manager.CellPhone</td>
                                <td>@manager.Enable</td>
                                <td>@manager.CreateTime</td>
                                <td>
                                    <a href="javascript:;" class="btn btn-xs btn-primary no-margin">详情</a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
            <!-- /.box-body -->
        </div>



    </div>
</div>
